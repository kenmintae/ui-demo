{
  "name": "z-styleguide",
  "version": "0.2.0",
  "zenefits": {
    "level": "app",
    "uiReviewer": "wachunga",
    "owner": "wachunga"
  },
  "scripts": {
    "start": "styleguidist server",
    "prebuild": "zrun node config/buildComponentStatus",
    "build": "zrun styleguidist build",
    "deploy": "EMBER_CLI_DEPLOY_REUSE_BUILD=true ember deploy production --verbose",
    "master-only-activate": "ember deploy:activate production --revision \"${BUILD_SHA:-$TRAVIS_COMMIT}\"",
    "lint": "zrun 'yarn tslint && yarn eslint && yarn stylelint && yarn tsCheck && yarn configCheck'",
    "stylelint": "zrun z-frontend-stylelint",
    "tsCheck": "zrun tsc --noEmit",
    "configCheck": "zrun node config/validateStyleguideConfig",
    "tslint": "zrun zcli tslintPackage",
    "eslint": "zrun z-frontend-eslint",
    "test": "zrun jest",
    "storybook": "start-storybook",
    "storybook:build": "zrun build-storybook"
  },
  "private": true,
  "engines": {
    "yarn": "^1.3.2",
    "node": "^10.14.1"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
    "react-docgen-typescript": "^1.12.1",
    "react-dom": "^16.6.3",
    "react-intl": "^2.3.0",
    "react-router-dom": "^4.1.2",
    "typescript": "3.2.2",
    "z-frontend-app-bootstrap": "*",
    "z-frontend-charts": "*",
    "z-frontend-chat": "*",
    "z-frontend-composites": "*",
    "z-frontend-data-manager": "*",
    "z-frontend-drag-and-drop": "*",
    "z-frontend-elements": "*",
    "z-frontend-forms": "*",
    "z-frontend-layout": "*",
    "z-frontend-tables": "*",
    "z-frontend-theme": "*",
    "zbase": "*"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "babel-jest": "^23.6.0",
    "babel-preset-z-frontend": "*",
    "ember-cli": "ember-cli/ember-cli#master",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-zenefits": "git+ssh://git@github.com/zenefits/ember-cli-deploy-zenefits.git#v1.2.4",
    "eslint-config-z-frontend": "*",
    "jest": "^23.6.0",
    "jest-enzyme": "^4.0.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-styleguidist": "^7.0.4",
    "stylelint-config-z-frontend": "*",
    "tslint": "^5.8.0",
    "typescript": "3.2.2",
    "z-frontend-jest": "*",
    "z-frontend-scripts": "*",
    "z-frontend-storybook-config": "*",
    "z-frontend-webpack": "*"
  }
}
